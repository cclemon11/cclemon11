<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Programme</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.0/css/bulma.min.css">
</head>


<style>
    .spaceTitle {
        padding-bottom: 24px;
    }

    .spaceCard {
        padding-left: 6px;
        padding-right: 6px;
    }

    .spaceBody {
        padding-left: 30px;
        padding-right: 30px;
    }

    .spaceContent {
        padding-left: 12px;
        padding-right: 12px;
        padding-top: 12px;
        padding-bottom: 12px;
    }
</style>


<body class="spaceBody">
    <div class="container">
        <div class="columns">
            <div class="spaceCard">
                <div class="row">
                    <% programmes.forEach( function(programme) { %>
                        <div class="card" style="width: 40rem;">
                            <div class="card-image">
                                <figure>
                                    <img src='<%= programme.imageurl %>' width="500" height="600"></img>

                                </figure>
                            </div>
                            <div class="card-content">
                                <div class="media-content">
                                    <div class="spaceTitle">
                                        <%= programme.programmename %>

                                    </div>
                                    <div class="spaceContent">
                                        <p class="subtitle is-6"><a>
                                            <%= programme.facultyname %>
                                               
                                        </p></a>
                                    </div>
                                </div>
                            </div>

                        </div>
                </div>
            </div>
            <br>
            <% }); %>

        </div>
    </div>

</body>